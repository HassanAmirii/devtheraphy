
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Resource {
  id               Int      @id @default(autoincrement())
  emotionCategory  String
  title             String
  type              String
  url               String
  tags              String
  shortDescription  String
}

model Prompt {
  id               Int      @id @default(autoincrement())
  emotionCategory  String
  text              String
}